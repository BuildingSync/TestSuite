<?xml version="1.0" encoding="UTF-8"?>
<auc:BuildingSync xmlns:auc="http://buildingsync.net/schemas/bedes-auc/2019"
	xsi:schemaLocation="http://buildingsync.net/schemas/bedes-auc/2019 https://raw.githubusercontent.com/BuildingSync/schema/1c73127d389b779c6b74029be72c6e9ff3187113/BuildingSync.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<auc:Facilities>
		<auc:Facility>
			<auc:Sites>
				<auc:Site>
					<auc:ClimateZoneType>
						<auc:ASHRAE>
							<auc:ClimateZone></auc:ClimateZone>
						</auc:ASHRAE>
						<!-- Choose a ClimateZone, or define a different ClimateZoneType besides ASHRAE -->
					</auc:ClimateZoneType>
					<auc:Buildings>
						<auc:Building ID="">
							<!-- Define ID for Building -->
							<auc:PremisesName></auc:PremisesName>
							<!-- PremisesName recommended for OpenStudio Results output -->
							<auc:BuildingClassification/>
							<!-- Define BuildingClassification -->
							<auc:OccupancyClassification/>
							<!-- Define OccupancyClassicitaion -->
							<auc:FloorsAboveGrade/>
							<auc:FloorsBelowGrade/>
							<!-- Define number of floors above / below grade-->
							<auc:FloorAreas>
								<auc:FloorArea>
									<auc:FloorAreaType>Gross</auc:FloorAreaType>
									<auc:FloorAreaValue></auc:FloorAreaValue>
									<!-- Simply fill in FloorAreaValue -->
								</auc:FloorArea>
							</auc:FloorAreas>
						</auc:Building>
					</auc:Buildings>
				</auc:Site>
			</auc:Sites>
			<auc:Systems/>
			<auc:Measures>
				<auc:Measure/>
			</auc:Measures>
			<!-- Able to leave Measures blank, just need elements there -->
			<auc:Reports>
				<auc:Report>
					<auc:Scenarios>
						<auc:Scenario ID="Baseline">
							<!-- Change scenario ID if desired -->
							<auc:ScenarioName/>
							<auc:ScenarioType>
								<auc:PackageOfMeasures>
									<auc:ReferenceCase IDref="Baseline"/>
									<!-- If scenario ID changed, make sure to match IDref of ReferenceCase to the new scenario ID -->
								</auc:PackageOfMeasures>
							</auc:ScenarioType>
						</auc:Scenario>
					</auc:Scenarios>
				</auc:Report>
			</auc:Reports>
		</auc:Facility>
	</auc:Facilities>
</auc:BuildingSync>
