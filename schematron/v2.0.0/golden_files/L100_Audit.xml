          <auc:LongDescription>This measure does something cheap</auc:LongDescription>
        </auc:Measure>

        <!-- Capital measure -->
        <auc:Measure>
          <!-- 6.1.6.b -->
          <auc:SystemCategoryAffected>Air Distribution</auc:SystemCategoryAffected>
          <!-- 6.1.6.a -->
          <auc:CustomMeasureName>Capital Measure</auc:CustomMeasureName>
          <!-- 6.1.6.a -->
          <auc:LongDescription>This measure does something expensive</auc:LongDescription>
        </auc:Measure>
