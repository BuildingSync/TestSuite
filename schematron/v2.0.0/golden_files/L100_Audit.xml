                    </auc:OccupancyLevel>
                  </auc:OccupancyLevels>
                  
                  <auc:TypicalOccupantUsages>
                    <!-- 6.1.1.1.g/5.3.4.b -->
                    <auc:TypicalOccupantUsage>
                      <auc:TypicalOccupantUsageValue>40</auc:TypicalOccupantUsageValue>
